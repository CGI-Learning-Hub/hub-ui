{
  "name": "@cgi-learning-hub/theme",
  "version": "0.0.1-dev",
  "private": false,
  "author": "CGI Learning-hub Team",
  "description": "@cgi-learning-hub/theme is a theme implemented with tailwindcss",
  "keywords": [
    "cgi-learning-hub",
    "tailwind",
    "tailwindcss",
    "hub",
    "hub-ui"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/CGI-OPEN-ENT-NG/hub-ui.git",
    "directory": "packages/theme"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "postcss ./src/index.css -o ./src/output.css",
    "build": "postcss ./src/index.css -o ./src/output.css && vite build",
    "build:watch": "vite build --watch"
  },
  "devDependencies": {
    "@mui/material": "5.15.7",
    "@tailwindcss/line-clamp": "0.4.4",
    "autoprefixer": "10.4.17",
    "postcss": "8.4.33",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "3.4.1"
  }
}
